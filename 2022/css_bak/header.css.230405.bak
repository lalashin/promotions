    position: relative;
    width: 321px;
    height: 84px;
    margin: 0 auto;
    /* background: #f2f2f2; */
}

header .top_left_bnr .swiper-button-next {
    height: 80px !important;
}

header .top_left_bnr .swiper-button-prev {
    height: 80px !important;
}

header .top_left_bnr .swiper-button-prev,
header .top_left_bnr .swiper-button-next{background:none}

header .top_left_bnr .swiper-button-next::after,
header .top_left_bnr .swiper-button-prev::after {    display: none;
}

header .top_left_bnr .top_bn_up1 {
    position: absolute;
    top: 15px;
    right: 0px;
    left: auto;
    color: #000;
    font-size: 16px;
}

header .top_left_bnr .top_bn_down1 {
    position: absolute;
    right: 0px;
    left: auto;
    top: 32px;
    color: #000;
    font-size: 16px;
}

header .top_right_bnr {
    width: 321px;
    height: 80px;
    position: absolute;
    top: 30px;
    right: 0;
    overflow: hidden;
    /* box-sizing:border-box;  border: 1px solid #000;*/
}

header .top_right_bnr .topR_swiper-container {
    position: relative;
    width: 100%;
    height: 80px;
    margin: 0 auto;
    /* background: #f2f2f2; */
}

header .top_right_bnr .swiper-button-next {
    height: 80px !important;
}

header .top_right_bnr .swiper-button-prev {
    height: 80px !important;
}

header .top_right_bnr .swiper-button-next::after,
header .top_right_bnr .swiper-button-prev::after {
    display: none;
}

header .top_right_bnr .top_bn_up {
    top: 15px;
    right: 0px;
    left: auto;
    color: #000;
    font-size: 16px;
}

header .top_right_bnr .top_bn_down {
    right: 0px;
