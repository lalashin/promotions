    header .mo_sns_area {
        display: inline-flex;
        margin-top: 5px;
        padding-left: 25px;
    }

    header .mo_sns_area>div {
        width: 35px;
        height: 35px;
        margin: 2px;
        text-align: center;
    }

    header .mo_family_link {
        width: 100%;
        text-align: center;
        padding: 20px 0 0 0;
    }

    header .mo_family_logo   {padding: 10px;}
    header .mo_family_logo > div { margin-bottom: 5px;}

    header .toolbar_wrap {
        display: block;
    }

    /* toolbar */ /*1116*/

    header .toolbar_wrap { display: block; width: 100%; position: fixed; z-index: 19; left: 0; bottom: 0; box-shadow: 0 0 20px 0 rgb(0 0 0 / 10%); transition:all .4s ease; -webkit-transition:all .4s ease; -ms-transition:all .4s ease; -moz-transition:all .4s ease; -o-transition:all .4s ease;}
    header .toolbar_wrap.on {  display: block; }
    header .toolbar_wrap.hide { bottom: -100%; }

    header .toolbar_wrap .mo_banner{ position: relative; background: #fff; text-align: center; min-height: 50px; line-height: 50px }
    header .toolbar_wrap .mo_banner .top_banner {  text-align: center; width: 100%; height: 100%; background-repeat: no-repeat; background-position: center; background-size:auto 100%;}
    header .toolbar_wrap .mo_banner .top_banner img { opacity: 0; visibility: hidden; }

    header .toolbar_wrap .swiper-pagination3 { position: absolute; z-index: 1; text-align: center; bottom: 1px !important;}
    header .toolbar_wrap .swiper-pagination3 .swiper-pagination-bullet  { width: 13px; height: 13px;  background: none;  border: 2px solid #fff;  opacity: 1; }
    header .toolbar_wrap .swiper-pagination3 .swiper-pagination-bullet + .swiper-pagination-bullet { margin-left: 10px; }
    header .toolbar_wrap .swiper-pagination3 .swiper-pagination-bullet-active { background: #fff }

    header .toolbar_wrap .mo_banner .banner_arr { position: absolute; z-index: 10; top: 50%; transform:translateY(-50%) ; -webkit-transform:translateY(-50%); -o-transform:translateY(-50%); -ms-transform:translateY(-50%); -moz-transform:translateY(-50%);opacity: 0.5; cursor:pointer;}
    header .toolbar_wrap .mo_banner .banner_arr.banner_prev3 { left: 10px; }
    header .toolbar_wrap .mo_banner .banner_arr.banner_next3 { right: 10px }
    header .toolbar_wrap .mo_banner .banner_arr img { max-width: 15px}
    header .toolbar_wrap .mo_banner .banner_arr:hover { opacity: 1}

    header .toolbar { background: #fff; }
    header .toolbar ul li { float: left; display: inline-block; width: 20%; }
    header .toolbar ul li a { display: block; padding: 12px 0; text-align: center; height: 70px; box-sizing: border-box; }
    header .toolbar ul li a span { display: block; text-align: center; font-size: 12px; color: #333; letter-spacing: -0.5px; }
    header .toolbar ul li.on a span { color: #164194; }
    header .toolbar ul li a .tools_icon svg { height: 26px; }
    header .toolbar ul li.li5 a .tools_icon { padding: 3px 0; }
    header .toolbar ul li.li5 a .tools_icon svg { height: 20px; }
    header .toolbar ul li.on a .tools_icon svg { fill : #164194;  }

}

