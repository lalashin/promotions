}

header .miraeij_logo>a>img {
    width: 251px;
    padding: 45px 0 45px;
}

header .top_left_bnr {
    width: 360px;
    height: 80px;
    position: absolute;
    top: 30px;
    left: 0;
    overflow: hidden;
    /*box-sizing:border-box;  border: 1px solid #000;*/
}

