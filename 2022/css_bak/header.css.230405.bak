/*Header*/

header .top_wrapper {
    width: 100%;
    box-sizing: border-box;
    border-bottom: 1px solid #003366;
}

header .top_wrapper.pc {
    display: block;
}

header .top_wrapper.mo {
    display: none;
}

header .top_area {
    width: 100%;
    max-width: 1176px;
    height: auto;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    padding-top: 14px;
}

header .topGnb_tab_button {
    display: block;
    display: flex;
    height: 35px;
}

header .topGnb_tab_button>li>a {
    font-size: 14px;
    display: block;
    color: #2b2b2b;
    padding: 0 20px;
    line-height: 34px;
    border-top-right-radius: 5px;
    border-top-left-radius: 5px;
    position: relative;
    margin-left: -1px;
}

header .topGnb_tab_button>li:nth-child(1)>a::after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 3px;
    background: #003366;
}

header .topGnb_tab_button>li:nth-child(2)>a::after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 3px;
    background: #013332;
}

header .topGnb_tab_button>li:nth-child(3)>a::after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 3px;
    background: #fe9901;
}

header .topGnb_tab_button>li:nth-child(1)>a {
    border-top: 1px solid #003366;
    border-left: 1px solid #003366;
    border-right: 1px solid #003366;
    border-bottom: 1px solid #003366;
    color: #003366;
    font-weight: 700;
    background: #fff;
}

header .topGnb_tab_button>li:nth-child(2)>a {
    border-top: 1px solid #013332;
    border-left: 1px solid #013332;
    border-right: 1px solid #013332;
    border-bottom: 1px solid #003366;
    color: #003366;
    font-weight: 700;
    background: #fff;
}

header .topGnb_tab_button>li:nth-child(3)>a {
    border-top: 1px solid #fe9901;
    border-left: 1px solid #fe9901;
    border-right: 1px solid #fe9901;
    border-bottom: 1px solid #003366;
    color: #003366;
    font-weight: 700;
    background: #fff;
}

header .topGnb_tab_button>li.on:nth-child(1)>a{
    line-height: 35px;
    border-bottom: none;
}
header .topGnb_tab_button>li.on:nth-child(2)>a{
    line-height: 35px;
    border-bottom: none;
}
header .topGnb_tab_button>li.on:nth-child(3)>a{
    line-height: 35px;
    border-bottom: none;
}


header .login_wrap {
    display: flex;
    justify-content: flex-end;
}

header .login_area>ul {
    display: flex;
}

header .login_area>ul>li>a {
    position: relative;
    line-height: 25px;
    display: block;
    padding: 0 20px 0 30px;
}

/* .login_area > ul > li > a::before{ content: ''; position: absolute; left: 0; bottom: 0; width: 30px; height: 30px; background: #000;} */
header .login_area>ul>li.i_login>a::before {
    content: "";
    position: absolute;
    left: 4px;
    bottom: 0;
    width: 28px;
    height: 28px;
    background: url(../images/icon/login-.png) no-repeat;
    background-size: 90%;
}

header .login_area>ul>li.i_logout>a::before {
    content: "";
    position: absolute;
    left: 4px;
    bottom: 0;
    width: 28px;
    height: 28px;
    background: url(../images/icon/logout-.png) no-repeat;
    background-size: 90%;
}


header .login_area>ul>li.i_join>a::before {
    content: "";
    position: absolute;
    left: 3px;
    bottom: 0;
    width: 28px;
    height: 28px;
    background: url(../images/icon/write.png) no-repeat;
    background-size: 90%;
}

header .login_area>ul>li.i_lecture>a::before {
    content: "";
    position: absolute;
    left: 4px;
    bottom: 0;
    width: 28px;
    height: 28px;
    background: url(../images/icon/lecture.png) no-repeat;
    background-size: 90%;
}

header .logo_wrapper {
    width: 100%;
}

header .logo_area {
    position: relative;
    width: 100%;
    max-width: 1176px;
    height: auto;
    margin: 0 auto;
    /*box-sizing:border-box;  border: 1px solid #000;*/
}

header .miraeij_logo {
