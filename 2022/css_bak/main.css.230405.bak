.pass_wrapper {width: 100%; height:auto; margin-bottom: 20px; background: url(../images/pass_bg_01.jpg) no-repeat; background-position: center; /*background-image: linear-gradient(to top, #37ecba 0%, #72afd3 100%);*/}
