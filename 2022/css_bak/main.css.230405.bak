/*middle*/
#mainBannerDiv {width:100%;}
.middleArea.main{width: 100%; margin: 0 auto; position: relative;}
.main_wrapper {position: relative; width: 1200px; padding: 0 12px; margin: 0 auto; margin-bottom: 20px; box-sizing: border-box; /*border: 1px solid #000;*/}
.wide_wrapper {position: relative; width: 100%; }

/**main pr area*/
.main_pr_area{display: flex; margin-bottom: 20px; justify-content: space-between;}
.main_big_slide { width: 580px; height: 250px; overflow: hidden; position: relative; }
.main_big_slide img, .main_off_slide img, .main_on_slide img {width: 100%;}

.swiper-main {position: relative;overflow:hidden;width: 100%;height: 100%; }
.swiper-pagination-tit {display: flex;font-size:13px ;justify-content: flex-start; text-align: center; }
.swiper-pagination-tit-active {  color: #333;  }
.swiper-pagination-tit-active span { color:#000 ;font-weight: bold;}
.swipertitle-wrap { position: absolute;font-size:13px ;bottom: 10px;left: 50%; transform: translateX(-50%);width: 40%; z-index: 1;cursor: pointer;}
.custom_bullet { width: 25%;    margin-right: 20px;    padding: 0;        transition: all .3s;  }
.custom_bullet span {position: relative;}
.custom_bullet span::after {content:''; position: absolute; width:1px; height:100%; top:0; right: -20px; background: #a1a1a1; z-index: 5; }
.custom_bullet:last-child { margin-right: 0; }
.custom_bullet:last-child span::after {width: 0;}

.box_border {position: relative;}
.box_border::after,
.box_border::before{content:''; position: absolute; width:100%; height:1px; left:0; background: #e5e5e5; z-index: 5;}
.box_border::before{top:0}
.box_border::after{bottom:0;}
.box_border > div::before,
.box_border > div::after{content:''; position: absolute; width:1px; height:100%; top:0; background: #e5e5e5; z-index: 5; }
.box_border > div::before{left:0}
.box_border > div::after{right:0}

.main_off_slide { width: 242px; height: 252px; overflow: hidden;}
.main_off_slide_tit {display: none;}
.main_off_slide .off_swiper-container{ position: relative; width: 242px; height: 252px; /*box-sizing:border-box; border: 1px solid #e5e5e5; */overflow: hidden;}
.main_off_slide .swiper-wrapper{ width: 100%;}
.main_off_slide .swiper-pagination-bullet {background-color: #333; margin: 0 10px;}
.off_info {display: none;}

.main_on_slide { width: 332px; height: 252px; overflow: hidden;}
