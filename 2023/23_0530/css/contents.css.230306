.request_pop.online {display: none;}
.request_pop.offline {display: none;}
.request_pop {   width: 100vw; height: 100vh; position: fixed; top: 0; left: 0; background: rgba(10, 10, 10, 0.8); z-index: 500  }
.request_pop .pop_wrap { background: #fff;  width: 100%; max-width:800px; height: 700px;  position: absolute; top: 50%; left: 50%;  transform:translate(-50%, -50%) ; -webkit-transform:translate(-50%, -50%); -o-transform:translate(-50%, -50%); -ms-transform:translate(-50%, -50%); -moz-transform:translate(-50%, -50%);/*border-bottom-left-radius: 40px;  border-top-right-radius: 40px;*/ overflow: hidden; box-sizing: border-box}

.request_pop .tit_wrap { background: #10264f; height: 80px; line-height: 80px; position: relative; padding: 0 30px;  }
.request_pop .tit_wrap > p {  font-size: 2.2rem;  line-height: 80px;  letter-spacing: -0.5px; color: #fff }
.request_pop .tit_wrap > a { display: block; width: 40px; height: 40px; border-radius: 50%; border: 1px solid #fff; text-align: center; line-height: 39px; box-sizing: border-box; position: absolute; right: 30px; top: 50%; margin-top: -20px}

.request_pop .desc_wrap{ padding: 35px 30px 55px 30px; height: 530px; overflow-y: auto;}
.request_pop .desc_wrap::-webkit-scrollbar {  width: 6px; }
.request_pop .desc_wrap::-webkit-scrollbar-thumb {  background-color: rgba(22, 65, 148, 0.2);  border-radius: 2.5px; }
.request_pop .desc_wrap::-webkit-scrollbar-track { border-radius: 2.5px;   }

.request_pop .desc_wrap .top { background: #fff;  border-bottom: 2px solid #164194; position: relative;  border-top-right-radius: 30px;  box-sizing: border-box; padding: 0 0 20px 0px;font-size: 1.5rem; line-height: 22px; letter-spacing: -0.5px;  color: #10264f;   font-weight: 500}
.request_pop .desc_wrap .btm .write > div { padding: 0px; border-bottom: 1px solid rgba(204, 204, 204, 0.5); color: #333 }
.request_pop .desc_wrap .btm .write > div:last-child { border-bottom: none; }
.request_pop .desc_wrap .btm .write > div > b { width: 170px; float: left;  font-size: 1.5rem;  line-height: 22px;   letter-spacing: -0.5px;  }
.request_pop .desc_wrap .btm .write > div > div {  width: calc(100% - 170px);  float: left; }
.request_pop .btn_box{ float: none; width: 100%; text-align: center !important; margin-top: 10px}

.request_pop .bbs .write { border-top: none;}
.request_pop .bbs .write th { padding:13px 25px}
.request_pop .bbs .write td { padding: 5px 10px; }
.request_pop .bbs .write tr .agree { padding: 0 20px; margin: 0px }
.request_pop .bbs .write tr .agree .txt_wrap { color: #333;  border: 1px solid #ccc; border-radius: 4px; width: 100%; height: 145px; margin: 0 auto; padding: 15px; font-size: 1.5rem; font-weight: 400; line-height: 22px;   letter-spacing: -0.3px; box-sizing: border-box;overflow-y: auto; }
.request_pop .bbs .write tr .agree .txt_wrap::-webkit-scrollbar {  width: 6px; }
.request_pop .bbs .write tr .agree .txt_wrap::-webkit-scrollbar-thumb {  background-color: rgba(22, 65, 148, 0.2);  border-radius: 2.5px; }
.request_pop .bbs .write tr .agree .txt_wrap::-webkit-scrollbar-track { border-radius: 2.5px;   }
.request_pop .bbs .write tr .agree .txt_wrap h3 { font-size: 1.8rem; font-weight: 700; line-height: 22px;   letter-spacing: -0.3px;   }
.request_pop .bbs .write tr .agree .txt_wrap li span { display: block; }
.request_pop .bbs .write tr .agree .check { margin: 5px 0; }
.request_pop span.icon { display: inline-block;  width: 20px;  height: 20px;   border-radius: 50%;  background: #164194;  text-align: center;    line-height: 19px;  margin-right: 0px;}
.request_pop span.icon img { margin-left: -1px; }
.request_pop  input[type="checkbox"] + label { width: 100%  !important;}
.request_pop .bbs .write td .t_red { font-size: 12px; color: red;}


