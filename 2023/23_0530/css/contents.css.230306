@media screen and (max-width: 960px){
	 #sub .bbs_wrap .linkform input[type="text"] {height:55px; line-height:55px; font-size:2rem;}
	 #sub .bbs_wrap .linkform input[type="text"]::placeholder{font-size:2rem;}
	 #sub .bbs_wrap .linkform input[type="button"]{padding-left:20px; height:55px; line-height:55px; font-size:3rem; letter-spacing:8px;}
}




@media screen and (max-width: 768px){


	#sub .bbs_wrap { padding-top: 0}
	#sub .bbs_wrap .board_area .tab_set a { width: 245px; }
	#sub .bbs_wrap .linkform { padding: 0 5%; }
	#sub .bbs_wrap .board_area { padding: 10% 10px 0 10px; }
	#sub .bbs_wrap .bbs .list_bg { padding: 20px 15px; }

	#sub .section2 .sec2_1 .txt_wrap .line_ab.line1 { top: 30.333%; }
	#sub .section2 .sec2_1 .txt_wrap .line_ab.line2 { top: 60.333%; }

}

@media screen and (max-width: 720px){
	#sub .pc {  display: none !important;}
	#sub .mo {  display: inline-block !important;}
}

@media screen and (max-width: 640px){


	#sub .bbs_wrap .linkform { padding: 0 2%; }
	#sub .bbs_wrap .linkform input[type="text"]{width:70%; padding:0 20px;}
	#sub .bbs_wrap .linkform input[type="button"]{width:30%;}

}


@media screen and (max-width: 480px){
	#sub .section6 .btm, #sub .section6 .btm .ri { margin-top: 25vw }

	#sub .bbs_wrap .linkform input[type="text"] {font-size:1.5rem;}
	#sub .bbs_wrap .linkform input[type="text"]::placeholder{font-size:1.5rem;}
	#sub .bbs_wrap .linkform input[type="button"]{font-size:2rem;}

	#sub .section2 .sec2_1 .txt_wrap .line_ab.line1 { top: 27.333% }
	#sub .section2 .sec2_1 .txt_wrap .line_ab.line2 { top: 57.333% }
}



