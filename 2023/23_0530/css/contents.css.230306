#sub .section .size{max-width: 1280px; padding:0; margin:0 auto; position: relative;}
#sub .section1{background: url('../images/bg-img1.png') top center no-repeat;}
#sub .section2{background: url('../images/bg-img2.png') top center no-repeat;}
#sub .section3{background: #fff;}
#sub .section4{background: #338bd6;}
#sub .section6{background: #eceef3;}
#sub .section5{background: #071b2a;}
#sub .section9{background: #071b2a;}
#sub .section7{background: url('../images/bg-img7.png') top center no-repeat;}
#sub .bbs_wrap .bbs .pagenate{padding:0 0 40px; margin:0 !important;}
#sub .pagenate li a{color:#fff;}

#sub .pc {  display: inline-block !important;}
#sub .ta {  display: none !important;}
#sub .mo {  display: none !important;}

#sub .sec_wrap { padding-bottom: 0px;}
#sub .section { width: 100%; text-align: center }


#sub .bbs_wrap {padding-bottom: 0px; padding-top:0px; box-sizing:border-box; position:relative;}
#sub .bbs_wrap .board_area{padding: 130px 30px 0 30px; box-sizing:border-box;}
#sub .bbs_wrap .board_area .tab_set { text-align: left;padding-left: 30px;}
#sub .bbs_wrap .board_area .tab_set a{display:inline-block; width:30%; }
#sub .bbs_wrap .board_area .tab_set span{display:inline-block; width:68%; color:#ff6666; font-size:18px; text-align: right; }

#sub .bbs_wrap .bbs .list_bg { padding: 45px 30px; }
#sub .bbs_wrap .bbs .list { background: #fff; }
#sub .bbs_wrap .bbs .list thead { border-top : none; border-bottom: 2px solid #000; }
#sub .bbs_wrap .bbs .list thead th { color: #000}
#sub .bbs_wrap .bbs .list tbody td.title{display:block; padding-left:0; margin-left:0;}
#sub .bbs_wrap .bbs .list tbody td.date::after{display:none;}
#sub .bbs_wrap .bbs .pagenate { margin: 40px auto 40px; }

#sub .bbs_wrap .linkform{padding:0 81px; box-sizing:border-box;}
#sub .bbs_wrap .linkform .inner{border:4px solid #ff6666; box-sizing: border-box; position:relative; box-sizing: border-box; font-size:0;}
#sub .bbs_wrap .linkform input::placeholder{font-size:29px; font-weight:300;}
#sub .bbs_wrap .linkform input[type="text"] {width:80%; border:none; height:78px; line-height:78px; padding:0 50px; font-size:2.9rem; color:#666666; background:#fff; }
#sub .bbs_wrap .linkform input[type="button"] {width:20%; padding-left:30px; border:none; background:none; height:78px; line-height: 78px; font-size:3.75rem; color:#fff; font-weight: 600; letter-spacing: 19px; background-color:#ff6666;}

.bbs .list tbody tr.top{background:#f1f6fc;}

/**************scrollout**********/


#sub .popup .pop_wrap { width: 90% !important; max-width: 470px; left: 50%; width: auto; min-height: auto;  border-radius: 0px; margin-top: 0; margin-left: 0; transform: translate(-50%, -50%); -webkit-transform: translate(-50%, -50%);-o-transform: translate(-50%, -50%); -ms-transform: translate(-50%, -50%); -moz-transform: translate(-50%, -50%); overflow: initial; }
#sub .popup .desc_wrap .top { background: #f4f6f9;  border-bottom: 2px solid #164194; position: relative;  border-top-right-radius: 30px;  box-sizing: border-box; padding: 20px 30px 20px 30px;font-size: 1.5rem; line-height: 22px; letter-spacing: -0.5px;  color: #10264f;   font-weight: 500}
#sub .popup .desc_wrap .btm > div { padding: 15px 20px 15px 20px; border-bottom: 1px solid rgba(204, 204, 204, 0.5); color: #333 }
#sub .popup .desc_wrap .btm > div:last-child { border-bottom: none; }
#sub .popup .desc_wrap .btm > div > b { width: 170px; float: left;  font-size: 1.5rem;  line-height: 22px;   letter-spacing: -0.5px;  }
#sub .popup .desc_wrap .btm > div > div {  width: calc(100% - 170px);  float: left; }
#sub .popup .desc_wrap .inid{height:40px;padding:8px 10px;border:1px solid #cccccc;line-height:1; max-width:300px;}

#sub .popup .desc_wrap .fileBox_wrap { position:relative; }
#sub .popup .desc_wrap .fileBtn{display:block; width:110px; height:40px; float:left;}
#sub .popup .desc_wrap .fileBox_wrap p.ptxt{clear: both;  display:block; margin-top:10px; color:#164194; font-weight: 400;}
#sub .popup .desc_wrap .fileBox .inputBox{float:left; box-sizing: border-box; }
#sub .popup .desc_wrap .fileBox input[type="text"]{float:left;overflow:hidden;box-sizing:border-box;height:32px;padding:8px 10px;border:1px solid #cccccc;line-height:1;background:#fafafa;  box-sizing: border-box; }
#sub .popup .desc_wrap .fileBox_wrap .inputBox{float:left;width: calc(100% - 170px);  box-sizing: border-box;}

#sub .popup .desc_wrap .fileBox p.ptxt{clear: both;  display:block; margin-top:10px; color:#164194; font-weight: 400;}
#sub .popup .desc_wrap .fileBtn{float:left;margin-left:5px;  box-sizing: border-box;}
#sub .popup .desc_wrap .fileBtn label{display:inline-block;width:150px;font-size:14px;line-height:40px; height:40px; letter-spacing:-1px;color:#f6f6f6;background:#757880;text-align:center; border-radius: 4px;  box-sizing: border-box;}
#sub .popup .desc_wrap .fileBtn input[type="file"]{visibility:hidden;width:0;height:0;padding:0;margin:0;}


.popup.cri_pop .pop_closebtn  { display: block;  width: 40px; height: 40px; border-radius: 50%; border: 1px solid #fff; text-align: center;  line-height: 39px; box-sizing: border-box; position: absolute; right: 0px; top: -50px; /*background: #10264f;*/}
.popup.cri_pop .imgs { display: none; }

#sub .popup .tit_wrap > p.p2,
#sub .popup .desc_wrap .top.top2 { display: none }
#sub .pop_transfer.type4 .tit_wrap > p,
#sub .pop_transfer.type4 .desc_wrap .top { display: none }
#sub .pop_transfer.type4 .tit_wrap > p.p2,
#sub .pop_transfer.type4 .desc_wrap .top.top2 { display: block }


